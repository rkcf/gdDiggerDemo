"language": "GDScript",
"path": "res://scenes/Digger.gd"
}, {
"base": "Node",
"class": "DiggerManager",
"language": "GDScript",
"path": "res://scenes/DiggerManager.gd"
}, {
"base": "Node",
"class": "Room2D",
"language": "GDScript",
"path": "res://scenes/Room2D.gd"
}, {
